<% for(var i=0; i < songs.length; i++) { %>
<li id="<%= songs[i].id %>">

    <div>

        <h4><%= songs[i].title %></h4>
        <em><%= songs[i].artist %></em>

        <ul class="actions">

            <li class="add"><a href="#">Add Song</a></li>
            <li class="play"><a href="#">Listen</a></li>
    
        </ul>

    </div>

</li>
<% } %>